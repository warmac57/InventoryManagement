<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>InventoryManagement</RootNamespace>
    <UseWindowsForms>true</UseWindowsForms>
    <StartupObject>InventoryManagement.Program</StartupObject>
    <AssemblyName>InventoryManagement</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DevExpress.Win" Version="24.2.6" />
    <PackageReference Include="DevExpress.Win.Grid" Version="24.2.6" />
    <PackageReference Include="DevExpress.Win.Navigation" Version="24.2.6" />
    <PackageReference Include="DevExpress.Win.Charts" Version="24.2.6" />
    <PackageReference Include="DevExpress.Data" Version="24.2.6" />
    <PackageReference Include="DevExpress.Printing.Core" Version="24.2.6" />
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="DMP-inventory-entity.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
